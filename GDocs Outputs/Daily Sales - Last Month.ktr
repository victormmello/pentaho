    <sql>-- Venda:
SELECT
CASE CONVERT(NVARCHAR(6), vp.data_venda, 112)
	WHEN CONVERT(NVARCHAR(6), DATEADD(m,-1,GETDATE())-1, 112) THEN 'MTD'
	WHEN CONVERT(NVARCHAR(6), DATEADD(m,-1,DATEADD(m,-1,GETDATE())-1), 112) THEN 'LM'
	WHEN CONVERT(NVARCHAR(6), DATEADD(yy,-1,DATEADD(m,-1,GETDATE())-1), 112) THEN 'LY'
END as anomes,
CONVERT(NVARCHAR,MAX(vp.data_venda),103) as ultima_venda,
LTRIM(RTRIM(COALESCE(NULLIF(f.filial,'E-COMMERCE'),'CAMBUI'))) as filial,
p.colecao,
LTRIM(RTRIM(REPLACE(p.grupo_produto,'_',''))) as grupo_produto,
COUNT(DISTINCT vp.ticket) as pedidos,
REPLACE(SUM(vp.qtde),'.',',') as itens,
REPLACE(SUM(vp.qtde*(vp.preco_liquido + vp.desconto_item)),'.',',') as preco_original_total,
REPLACE(SUM(vp.qtde*(v.valor_pago/v.valor_venda_bruta*vp.preco_liquido)),'.',',') as preco_final_total,
REPLACE(SUM(vp.qtde*(v.valor_pago/v.valor_venda_bruta*vp.preco_liquido)),'.',',') as receita_total,
REPLACE(SUM(c.preco1*vp.qtde),'.',',') as custo_total
FROM loja_venda_produto vp
INNER JOIN loja_venda v on
	v.ticket = vp.ticket and
	vp.codigo_filial = v.codigo_filial and
	v.data_venda = vp.data_venda
INNER JOIN FILIAIS f on f.cod_filial = vp.codigo_filial
INNER JOIN produtos p on p.produto = vp.produto
INNER JOIN produtos_precos c on
	p.produto = c.produto and
	c.codigo_tab_preco = 02 -- 02 custo
WHERE
vp.qtde > 0 and
vp.preco_liquido > 0 and
CONVERT(NVARCHAR(6), vp.data_venda, 112) IN (
	CONVERT(NVARCHAR(6), DATEADD(m,-1,GETDATE())-1, 112), -- Current Month
	CONVERT(NVARCHAR(6), DATEADD(m,-1,DATEADD(m,-1,GETDATE())-1), 112), -- Last Month
	CONVERT(NVARCHAR(6), DATEADD(yy,-1,DATEADD(m,-1,GETDATE())-1), 112) -- Last Year
)
GROUP BY
CASE CONVERT(NVARCHAR(6), vp.data_venda, 112)
	WHEN CONVERT(NVARCHAR(6), DATEADD(m,-1,GETDATE())-1, 112) THEN 'MTD'
	WHEN CONVERT(NVARCHAR(6), DATEADD(m,-1,DATEADD(m,-1,GETDATE())-1), 112) THEN 'LM'
	WHEN CONVERT(NVARCHAR(6), DATEADD(yy,-1,DATEADD(m,-1,GETDATE())-1), 112) THEN 'LY'
END,
CASE WHEN CONVERT(NVARCHAR(6), vp.data_venda, 112) = CONVERT(NVARCHAR(6), DATEADD(m,-1,GETDATE())-1, 112) THEN CONVERT(NVARCHAR,vp.data_venda,103) END,
LTRIM(RTRIM(COALESCE(NULLIF(f.filial,'E-COMMERCE'),'CAMBUI'))),
p.colecao,
LTRIM(RTRIM(REPLACE(p.grupo_produto,'_','')))

UNION

-- Venda E-COMMMERCE:
SELECT
CASE CONVERT(NVARCHAR(6), f.emissao, 112)
	WHEN CONVERT(NVARCHAR(6), DATEADD(m,-1,GETDATE())-1, 112) THEN 'MTD'
	WHEN CONVERT(NVARCHAR(6), DATEADD(m,-1,DATEADD(m,-1,GETDATE())-1), 112) THEN 'LM'
	WHEN CONVERT(NVARCHAR(6), DATEADD(yy,-1,DATEADD(m,-1,GETDATE())-1), 112) THEN 'LY'
END as anomes,
CONVERT(NVARCHAR,MAX(f.emissao),103) as ultima_venda,
'E-COMMERCE' as filial,
p.colecao,
LTRIM(RTRIM(REPLACE(p.grupo_produto,'_',''))) as grupo_produto,
COUNT(DISTINCT fp.nf_saida + fp.filial) as pedidos,
REPLACE(SUM(fp.qtde),'.',',') as itens,
REPLACE(SUM(fp.valor + fp.desconto_item),'.',',') as preco_original_total,
REPLACE(SUM(fp.valor),'.',',') as preco_final_total,
REPLACE(SUM(f.valor_total/f.valor_sub_itens*fp.valor),'.',',') as receita_total,
REPLACE(SUM(c.preco1*fp.qtde),'.',',') as custo_total
FROM faturamento_prod fp
INNER JOIN faturamento f on
	f.nf_saida = fp.NF_SAIDA and
	fp.filial = f.filial
INNER JOIN produtos p on p.produto = fp.produto
INNER JOIN produtos_precos c on
	p.produto = c.produto and
	c.codigo_tab_preco = 02 -- 02 custo
WHERE
fp.qtde > 0 and
f.valor_sub_itens > 0 and
fp.serie_nf = 2 and
f.filial = 'e-commerce' and
CONVERT(NVARCHAR(6), f.emissao, 112) IN (
	CONVERT(NVARCHAR(6), DATEADD(m,-1,GETDATE())-1, 112), -- Current Month
	CONVERT(NVARCHAR(6), DATEADD(m,-1,DATEADD(m,-1,GETDATE())-1), 112), -- Last Month
	CONVERT(NVARCHAR(6), DATEADD(yy,-1,DATEADD(m,-1,GETDATE())-1), 112) -- Last Year
)
GROUP BY
CASE CONVERT(NVARCHAR(6), f.emissao, 112)
	WHEN CONVERT(NVARCHAR(6), DATEADD(m,-1,GETDATE())-1, 112) THEN 'MTD'
	WHEN CONVERT(NVARCHAR(6), DATEADD(m,-1,DATEADD(m,-1,GETDATE())-1), 112) THEN 'LM'
	WHEN CONVERT(NVARCHAR(6), DATEADD(yy,-1,DATEADD(m,-1,GETDATE())-1), 112) THEN 'LY'
END,
CASE WHEN CONVERT(NVARCHAR(6), f.emissao, 112) = CONVERT(NVARCHAR(6), DATEADD(m,-1,GETDATE())-1, 112) THEN CONVERT(NVARCHAR,f.emissao,103) END,
p.colecao,
LTRIM(RTRIM(REPLACE(p.grupo_produto,'_','')))</sql>
