    <connection>Linx</connection>
    <sql>SELECT
LTRIM(RTRIM(dpc.produto)) as produto,
MAX(LTRIM(RTRIM(p.colecao))) as colecao,
MAX(COALESCE(ccc.categoria,cct.categoria,ccs.categoria,cc.categoria,'Outros')) as categoria,
MAX(LTRIM(RTRIM(p.desc_produto))) as descricao,
MAX(dpc.quantidade_estoque_filial) as estoque,
MAX(dpc.disponibilidade_filial) as disponibilidade,
MAX(pf.revista) as revista,
MAX(pf.pagina) as pagina,
MAX(CASE WHEN pos.data_postagem &lt;= GETDATE() THEN pos.data_postagem END) as data_ultima_postagem,
MAX('https://www.marciamello.com.br/' + REPLACE(LOWER(LTRIM(RTRIM(p.desc_produto))),' ','-') + '-' + REPLACE(LTRIM(RTRIM(p.produto)),'.','-') + '/p') as url
FROM bi_disponibilidade_produto_cor dpc
INNER JOIN produtos p on p.produto = dpc.produto
LEFT JOIN bi_categorizacao_categoria ccc on
	ccc.grupo_produto = p.grupo_produto and
	ccc.cod_categoria = p.cod_categoria and
	ccc.subgrupo_produto is null and
	ccc.tipo_produto is null
LEFT JOIN bi_categorizacao_categoria cct on
	cct.grupo_produto = p.grupo_produto and
	cct.tipo_produto = p.tipo_produto and
	cct.subgrupo_produto is null and
	cct.cod_categoria is null
LEFT JOIN bi_categorizacao_categoria ccs on
	ccs.grupo_produto = p.grupo_produto and
	ccs.subgrupo_produto = p.subgrupo_produto and
	ccs.tipo_produto is null and
	ccs.cod_categoria is null
LEFT JOIN bi_categorizacao_categoria cc on
	cc.grupo_produto = p.grupo_produto and
	cc.subgrupo_produto is null and
	cc.tipo_produto is null and
	cc.cod_categoria is null
LEFT JOIN bi_produto_foto pf on pf.produto = dpc.produto
LEFT JOIN bi_postagem pos on pos.produto = dpc.produto
WHERE
dpc.filial = 'e-commerce'
and dpc.produto IN (select dv.produto from bi_disponivel_vtex dv)
and quantidade_estoque_filial >= 4
and disponibilidade_filial >= 0.7
-- and COALESCE(pos.data_postagem,'1900-01-01') &lt; GETDATE()-60
GROUP BY
dpc.produto</sql>
